{
  "$schema" : "https://flagd.dev/schema/v0/flags.json",
  "flags" : {
    "basic-feature-toggle" : {
      "state" : "DISABLE",
      "defaultVariant" : "off",
      "variants" : {
        "on" : "ON",
        "off" : "OFF"
      },
      "targeting" : [ ]
    },
    "personalized-greeting" : {
      "state" : "ENABLED",
      "defaultVariant" : "hello",
      "variants" : {
        "hello" : "Hello!",
        "welcome" : "Welcome!",
        "hi" : "Hi there!"
      },
      "targeting" : [ {
        "if" : {
          "user_country" : "US"
        },
        "variant" : "welcome"
      }, {
        "if" : {
          "user_segment" : "premium"
        },
        "variant" : "hi"
      } ]
    }
  }
}